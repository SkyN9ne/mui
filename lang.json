{
  "lang": "mui",
  "version": "0.1.2",
  "tokens": [
    {
      "type": "nodeOpener",
      "pattern": "[a-zA-z]+\\s*\\(",
      "flags": ["m"]
    }, 
    {
      "type": "nodeCloser",
      "pattern": "\\)"
    },
    {
      "type": "string",
      "pattern": "'.*'"
    },
    {
      "type": "prop",
      "pattern": "[a-zA-z]+\\s*:",
      "flags": ["m"]
    },
    {
      "type": "newLine",
      "pattern":"\\n"
    },
    {
      "type": "comma",
      "pattern": ","
    }
  ]
}
